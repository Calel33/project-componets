{
  "name": "project-componets",
  "version": "1.0.0",
  "description": "Production-ready React component library with TypeScript and Tailwind CSS",
  "type": "module",
  "exports": {
    "./styles.css": "./styles.css",
    "./digital-health": {
      "import": "./dist/digital-health/index.mjs",
      "require": "./dist/digital-health/index.cjs"
    },
    "./payment": {
      "import": "./dist/payment/index.mjs",
      "require": "./dist/payment/index.cjs"
    },
    "./dashboard-sidebar": {
      "import": "./dist/dashboard-sidebar/index.mjs",
      "require": "./dist/dashboard-sidebar/index.cjs"
    },
    "./defi-earn": {
      "import": "./dist/defi-earn/index.mjs",
      "require": "./dist/defi-earn/index.cjs"
    },
    "./auth": {
      "import": "./dist/auth/index.mjs",
      "require": "./dist/auth/index.cjs"
    },
    "./hooks": {
      "import": "./dist/hooks/index.mjs",
      "require": "./dist/hooks/index.cjs"
    }
  },
  "files": [
    "dist",
    "styles.css",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "react",
    "components",
    "typescript",
    "tailwind",
    "ui-library",
    "healthcare",
    "dashboard",
    "payment",
    "web3",
    "design-system"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Calel33/project-componets.git"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:lib": "tsup",
    "preview": "vite preview",
    "prepublishOnly": "npm run build:lib"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "dependencies": {
    "chart.js": "^4.5.0",
    "lucide-react": "^0.344.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.19",
    "postcss": "^8.4.38",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwindcss": "^3.4.1",
    "tsup": "^8.5.0",
    "typescript": "^5.2.2",
    "vite": "^7.1.7"
  }
}
